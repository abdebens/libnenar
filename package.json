{
  "name": "libnear",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.css -w",
    "concat:css": "concat -o css/style.concat.css css/normalize.css css/all.min.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all  prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdebens/libnenar.git"
  },
  "author": "abdebens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abdebens/libnenar/issues"
  },
  "homepage": "https://github.com/abdebens/libnenar#readme",
  "devDependencies": {
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  },
  "dependencies": {
    "concat": "^1.0.3",
    "scrollreveal": "^4.0.9"
  }
}
